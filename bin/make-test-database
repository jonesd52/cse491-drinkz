#! /usr/bin/env python
import sys
import _mypath

from drinkz import db
from drinkz import build_db

build_db.Build_Database()


def main(args):
   if len(args) > 2:
        print >> sys.stderr, 'Usage: %s [file name]'
	return -1
	
   filename = args[1]
    
   db.save_db(filename)

if __name__ == '__main__':
   main(sys.argv)